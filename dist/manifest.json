{
    "name": "Uber price",
    "version": "1.0",
    "description": "Find out how much an uber would cost",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "tabs",
        "geolocation"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "index.html"
    }
}
